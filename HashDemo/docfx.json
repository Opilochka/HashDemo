{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "metadata": [
    {
      "src": [
        {
          "files": [ "*.csproj" ],
          "src": "."
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "api/**.yml", "toc.yml", "*.md" ]
      }
    ],
    "output": "_site",
    "template": ["default"]
  }
}